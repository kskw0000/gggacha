// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model GachaSetting {
  id             Int   @id
  wins           Int
  rolls          Int
  winProbability Float
}

model WinCode {
  winCode  String  @id
  issuedAt String
  userId   String?
}

model UserToken {
  id          String  @id
  userId      String?
  accessToken String
}
